            <!-- BOTOES DE NAVEGACAO -->
            <Button
                android:id="@+id/btnContinuar2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/btn_continue"
                android:textColor="@color/white"
                android:backgroundTint="@color/primary_500"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="150dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <Button
                android:id="@+id/btnVoltar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/btn_back"
                android:textColor="@color/white"
                android:backgroundTint="@color/primary_700"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="15dp"
                android:layout_marginEnd="200dp"/>

            <!-- TELA ONDE MOSTRA O PROGRESSO DO CADASTRO -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clyTelaCarregando"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:foreground="@color/darken_1"
                android:visibility="invisible">
                <ProgressBar
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:indeterminateTint="@color/triadic1_400"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <!-- MENU POR ONDE MOSTRA A FOTO DE PERFIL COM MAIS DETALHES -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clySuaFoto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/darken_1"
                android:visibility="invisible"
                android:enabled="false">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="320dp"
                    android:layout_height="370dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="@drawable/round_box2"
                    android:backgroundTint="@color/analogous2_800"
                    android:foreground="@drawable/round_box2_dashed_1">
                    <LinearLayout
                        android:id="@+id/llySuaFoto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginTop="10dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/title_yourPhoto"
                            android:textStyle="bold"
                            android:textColor="@color/white"
                            android:textSize="20sp"
                            android:layout_gravity="center_horizontal"/>
                        <LinearLayout
                            android:layout_width="230dp"
                            android:layout_height="230dp"
                            android:gravity="center"
                            android:background="@drawable/round_box2"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="20dp">
                            <ImageView
                                android:id="@+id/imvFotoBig"
                                android:layout_width="220dp"
                                android:layout_height="220dp"
                                android:src="@drawable/empty_profile"
                                android:foreground="@drawable/layout_borda_foto_2a"
                                android:contentDescription="@string/content_yourPhoto"/>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_horizontal"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/llySuaFoto">
                        <Button
                            android:id="@+id/btnSairFoto"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/btn_quit"
                            android:backgroundTint="@color/primary_700"
                            android:layout_marginEnd="10dp"/>
                        <Button
                            android:id="@+id/btnImportarFoto"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/btn_importPhoto"
                            android:backgroundTint="@color/primary_400"
                            android:layout_marginStart="10dp"/>
                    </LinearLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

